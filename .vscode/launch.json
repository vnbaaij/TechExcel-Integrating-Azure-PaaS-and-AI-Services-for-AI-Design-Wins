{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Attach to .NET Functions",
        "type": "coreclr",
        "request": "attach",
        "processId": "${command:azureFunctions.pickProcess}"
      },
      {
        "name": "Python:Streamlit",
        "type": "python",
        "request": "launch",
        "module": "streamlit",
        "console": "integratedTerminal",
        "justMyCode": true,
        "args": [
             "run",
             "${file}",
             
        ]
    }
    ]
  }